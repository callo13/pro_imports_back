{
  "kind": "collectionType",
  "collectionName": "import_products",
  "info": {
    "singularName": "import-product",
    "pluralName": "import-products",
    "displayName": "ImportProduct"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "images_url": {
      "type": "text"
    },
    "mise_en_avant": {
      "type": "integer",
      "default": 0
    },
    "images_file": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "details": {
      "type": "blocks"
    },
    "pastille": {
      "type": "component",
      "component": "pastille.pastille",
      "repeatable": true
    }
  }
}
